# Server Configuration
PORT=3000

# Authentication
# Required: API key for authenticating proxy requests
PROXY_API_KEY=your-secure-api-key-here

# Timeouts
# Request timeout in milliseconds (default: 5 minutes)
REQUEST_TIMEOUT_MS=300000

# Logging
# Log level: debug, info, warn, error
LOG_LEVEL=info

# Worker Pool (Phase 2)
# Number of concurrent Claude processes
WORKER_CONCURRENCY=2

# Maximum requests in queue before rejecting with 429
MAX_QUEUE_SIZE=100

# Maximum time a request can wait in queue (milliseconds)
QUEUE_TIMEOUT_MS=60000
